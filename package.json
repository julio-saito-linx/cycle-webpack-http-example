{
  "name": "cycle-webpack-http-example",
  "version": "0.0.1",
  "description": "Cycle.js + webpack",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/saitodisse/cycle-webpack-http-example"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Julio M Saito",
  "license": "MIT",
